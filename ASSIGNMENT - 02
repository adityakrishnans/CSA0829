------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

17. def remove_duplicates(lst):
    result = []
    for item in lst:
        if item not in result:
            result.append(item)
    return result

my_list = [1, 2, 2, 3, 1, 4, 3]
unique_list = remove_duplicates(my_list)
print("List without duplicates:", unique_list)

